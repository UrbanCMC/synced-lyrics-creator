<Application x:Class="SyncedLyricsCreator.App"
  xmlns="https://github.com/avaloniaui"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:fa="using:FluentAvalonia.Styling"
  xmlns:local="using:SyncedLyricsCreator">
  <Application.DataTemplates>
    <local:ViewLocator />
  </Application.DataTemplates>

  <Application.Styles>
    <fa:FluentAvaloniaTheme />
    <StyleInclude Source="avares://SyncedLyricsCreator/Assets/Styles/AppColors.axaml" />
    <StyleInclude Source="avares://SyncedLyricsCreator/Assets/Styles/ControlStyles.axaml" />
  </Application.Styles>
</Application>
